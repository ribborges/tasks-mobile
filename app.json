{
  "expo": {
    "name": "Tasks",
    "slug": "tasks-mobile",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ribborges.tasks"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#6366f1",
          "image": "./assets/images/splash-icon.png",
          "dark": {
            "backgroundColor": "#3730a3",
            "image": "./assets/images/splash-icon-dark.png"
          },
          "imageWidth": 200,
          "resizeMode": "contain"
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "280c1efe-bdbf-4fe8-bbb9-9b9b7281f231"
      }
    }
  }
}
